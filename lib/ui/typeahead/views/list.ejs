<% if(this.length) { %>
	<% for(var i=0;i<this.length;i++) { %>
		<% var item = this[i], newGroup = item.group && (i === 0 || this[i - 1].group !== item.group); %>
		<% if(newGroup) { %>
			<li><button disabled class="group"><%== item.group %></button><ul class="group-list">
		<% } %>
			<li><button data-value="<%== item.name %>"><%== item.name %></button></li>
		<% if(newGroup){ %>
			</ul></li>
		<% } %>
	<% } %>
<% } %>

<li style="display:<%= !this.length ? 'block' : 'none' %>"><button disabled>No items</button></li>
